{
  "name": "wrap-artist",
  "version": "0.0.2",
  "description": "An node wrapper module to wrap async callbacks into Promises to work with async/await",
  "main": "build/index.js",
  "scripts": {
    "examples": "babel examples --out-dir built-examples",
    "build": "babel index.js --out-dir build",
    "prepublish": "npm run build",
    "test": "mocha --recursive --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/toutapp/wrap-artist.git"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  },
  "keywords": [
    "async-to-promise",
    "callbacks",
    "promise",
    "es7",
    "async"
  ],
  "author": "ToutApp Engineering <engineering@toutapp.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toutapp/wrap-artist/issues"
  },
  "homepage": "https://github.com/toutapp/wrap-artist#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  }
}
